<div class="normal-page">
	<div class="container">
	  <div class="pop-up">
			<div class="well">
	  		<div class="row">

					<p id="notice"><%= notice %></p>

					<p>
					  <strong>Firstname:</strong>
					  <%= @contact.firstname %>
					</p>

					<p>
					  <strong>Lastname:</strong>
					  <%= @contact.lastname %>
					</p>

					<p>
					  <strong>Email:</strong>
					  <%= @contact.email %>
					</p>

					<p>
					  <strong>Language:</strong>
					  <%= @contact.language %>
					</p>

					<%= link_to 'Edit', edit_contact_path(@contact) %> |
					<%= link_to 'Back', contacts_path %>

				</div>
			</div>
		</div>
	</div>
</div>